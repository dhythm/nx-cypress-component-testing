{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-cypress-component-testing/components": [
        "libs/components/src/index.ts"
      ],
      "@nx-cypress-component-testing/icons": ["libs/icons/src/index.ts"],
      "@nx-cypress-component-testing/my-plugin": [
        "libs/my-plugin/src/index.ts"
      ],
      "@nx-cypress-component-testing/something": [
        "libs/something/src/index.ts"
      ],
      "@nx-cypress-component-testing/ws-generated-components": [
        "libs/ws-generated-components/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
